<h1>Editing contact</h1>

<%= form_for(@contact, :url => params.include?(:company_id) ? admin_company_contact_path(@company, @contact) : admin_vendor_contact_path(@vendor, @contact), method: "PUT") do |f| %>
  <%= render 'form', f: f %>
<% end %>

<%= link_to 'Back', params.include?(:company_id) ? admin_company_contacts_path : admin_vendor_contacts_path %>
