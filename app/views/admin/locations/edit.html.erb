<h1>Editing location</h1>

<%= form_for(@location, :url => params.include?(:company_id) ? admin_company_location_path(@company, @location) : admin_vendor_location_path(@vendor, @location), method: "PUT") do |f| %>
  <%= render 'form', f: f %>
<% end %>

<%= link_to 'Back', params.include?(:company_id) ? admin_company_locations_path : admin_vendor_locations_path %>
