# from old website to new website

RewriteCond %{QUERY_STRING} ^id=([0-9]+)-(.*)$
RewriteRule ^feedbackform.php$		/dashboard/feedback/?oid=%1 [R=301,L]

RewriteCond %{QUERY_STRING} ^id=([0-9]+)-(.*)$
RewriteRule ^feedbackformusers.php$		/dashboard/feedback/?oid=%1 [R=301,L]

RewriteRule ^calendar/$		/dashboard/calendar/ [R=301,L]
RewriteRule ^calendar$		/dashboard/calendar/ [R=301,L]
RewriteRule ^calendar/calendar.php$		/dashboard/calendar/ [R=301,L]
RewriteRule ^accounts/login.php$		/login.php [R=301,L]




