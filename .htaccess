RewriteRule ^dashboard/cancel/?$		/cancel.php [QSA,L]
RewriteRule ^dashboard/user-mgmt/?$		/user-mgmt.php [QSA,L]
RewriteRule ^dashboard/website-mgmt/?$		/website-mgmt.php [QSA,L]
