ErrorDocument 404 /error.php?404

Options -Indexes

RewriteEngine On

php_value post_max_size 100M
php_value upload_max_filesize 100M

# now manageable in c2me config file
#RewriteCond %{HTTP_HOST} ^www.cater2\.me [NC] 
#RewriteRule (.*) http://cater2.me/$1 [QSA,R=301,L]

RewriteCond %{HTTP_HOST} ^www.cater2me\.info [NC] 
RewriteRule (.*) http://cater2.me/$1 [QSA,R=301,L]
RewriteCond %{HTTP_HOST} ^cater2me\.info [NC] 
RewriteRule (.*) http://cater2.me/$1 [QSA,R=301,L]


# public website

RewriteRule ^about/?$			/about.php [QSA,L]
RewriteRule ^contact/?$			/contact.php [QSA,L]

RewriteRule ^login/?$			/login.php [QSA,L]
RewriteRule ^logout/?$			/logout.php [QSA,L]
RewriteRule ^signup/?$		/signup.php [QSA,L]
RewriteRule ^jobs/?$		/jobs.php [QSA,L]
RewriteRule ^event-spotlight/?$		/event-spotlight.php [QSA,L]
RewriteRule ^pwd-recovery/?$		/pwd-recovery.php [QSA,L]
RewriteRule ^betakitchen/?$			/betakitchen.php [QSA,L]
RewriteRule ^referral/?$			/referral_2.php [QSA,L]
RewriteRule ^clients/eventbrite/vendors/?$		/clients/eventbrite/vendors.php [QSA,L]


RewriteRule ^favicon.ico$			/template/favicon.ico [R=301,L]



# extranet

